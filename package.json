{
  "name": "kolotilshik",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(astro)\"",
